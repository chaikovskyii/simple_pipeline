      {
        "id": 4,
        "title": "Orders by Day of Week",
        "type": "barchart",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "P72C3179523E80511"
            },
            "format": "table",
            "rawSql": "SELECT TO_CHAR(order_date, 'Day') as day_name, COUNT(*) as orders FROM orders WHERE order_date >= $__timeFrom() AND order_date <= $__timeTo() GROUP BY EXTRACT(DOW FROM order_date), TO_CHAR(order_date, 'Day') ORDER BY EXTRACT(DOW FROM order_date)",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "options": {
          "orientation": "auto",
          "barWidth": 0.97,
          "groupWidth": 0.7,
