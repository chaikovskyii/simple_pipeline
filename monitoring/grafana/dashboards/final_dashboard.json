      {
        "id": 5,
        "title": "New vs Returning Customers",
        "type": "piechart",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "P72C3179523E80511"
            },
            "format": "table",
            "rawSql": "WITH customer_history AS (SELECT customer_email, MIN(order_date) as first_order FROM orders GROUP BY customer_email), time_range_orders AS (SELECT o.customer_email, o.order_date, ch.first_order FROM orders o JOIN customer_history ch ON o.customer_email = ch.customer_email WHERE o.order_date >= $__timeFrom() AND o.order_date <= $__timeTo()) SELECT CASE WHEN first_order >= $__timeFrom() THEN 'New Customer' ELSE 'Returning Customer' END as customer_type, COUNT(DISTINCT customer_email) as count FROM time_range_orders GROUP BY customer_type",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 16},
        "options": {
          "reduceOptions": {
            "values": true,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right"
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "unit": "short"
          }
        }
      },
